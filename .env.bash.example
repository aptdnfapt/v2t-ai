# Voice AI Bash Configuration

# Gemini API Configuration
# Multiple API keys separated by commas - will be used in round-robin
GEMINI_API_KEYS="YOUR_API_KEY_1,YOUR_API_KEY_2,YOUR_API_KEY_3"

# Rotate to next key after this many requests
KEY_ROTATION_COUNT=3

# Gemini model settings
GEMINI_MODEL_NAME="gemini-2.5-flash"
GEMINI_PROMPT_TEXT="Transcribe this audio accurately and quickly."

# Audio Recording Settings
ARECORD_DEVICE="default"
ARECORD_FORMAT="S16_LE"
ARECORD_RATE="16000"
ARECORD_CHANNELS="1"

# Audio Processing Settings
# Speed up audio if larger than this many MB
SPEEDUP_THRESHOLD=5
# Speed up factor when audio is too large
SPEEDUP_FACTOR=1.5
# Maximum allowed audio size in MB
MAX_AUDIO_SIZE=25